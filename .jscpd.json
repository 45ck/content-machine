{
  "threshold": 5,
  "reporters": ["console", "json"],
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/vendor/**",
    "**/templates/**",
    "**/connectors/**",
    "**/.git/**",
    "tests/**",
    "**/*.test.ts",
    "**/*.spec.ts"
